<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Simple Video Editor</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            margin: 0;
            padding: 0;
            background-color: #f4f4f4;
        }
        .container {
            width: 80%;
            margin: 0 auto;
            padding-top: 20px;
        }
        video {
            width: 100%;
            max-width: 600px;
            margin: 20px 0;
            border: 1px solid #ccc;
            border-radius: 4px;
        }
        .controls {
            text-align: center;
        }
        input[type="range"] {
            width: 80%;
            margin: 10px 0;
        }
        .button-group {
            margin-top: 20px;
        }
        button {
            padding: 10px 20px;
            margin: 0 10px;
            background-color: #333;
            color: white;
            border: none;
            cursor: pointer;
        }
        button:hover {
            background-color: #555;
        }
    </style>
</head>
<body>

    <div class="container">
        <h1>Simple Video Editor</h1>
        <video id="videoElement" controls>
            <source src="sample-video.mp4" type="video/mp4">
            Your browser does not support the video tag.
        </video>

        <div class="controls">
            <label for="startTime">Start Time:</label>
            <input type="range" id="startTime" min="0" max="100" value="0">
            <span id="startTimeValue">0</span> seconds

            <br>

            <label for="endTime">End Time:</label>
            <input type="range" id="endTime" min="0" max="100" value="100">
            <span id="endTimeValue">100</span> seconds
        </div>

        <div class="button-group">
            <button id="playButton">Play</button>
            <button id="trimButton">Trim Video</button>
        </div>
    </div>

    <script>
        // Get references to the video element and the controls
        const video = document.getElementById('videoElement');
        const startTimeSlider = document.getElementById('startTime');
        const endTimeSlider = document.getElementById('endTime');
        const startTimeValue = document.getElementById('startTimeValue');
        const endTimeValue = document.getElementById('endTimeValue');
        const playButton = document.getElementById('playButton');
        const trimButton = document.getElementById('trimButton');

        // Update the start time value when the slider is changed
        startTimeSlider.addEventListener('input', function() {
            startTimeValue.textContent = startTimeSlider.value;
        });

        // Update the end time value when the slider is changed
        endTimeSlider.addEventListener('input', function() {
            endTimeValue.textContent = endTimeSlider.value;
        });

        // Function to play the video from the start time
        playButton.addEventListener('click', function() {
            const start = parseFloat(startTimeSlider.value);
            const end = parseFloat(endTimeSlider.value);

            video.currentTime = start; // Start at the selected start time
            video.play();

            // Stop the video at the selected end time
            const checkEndTime = setInterval(function() {
                if (video.currentTime >= end) {
                    video.pause();
                    clearInterval(checkEndTime);
                }
            }, 50);
        });

        // Function to trim the video (not actual trimming, just controls playback)
        trimButton.addEventListener('click', function() {
            const start = parseFloat(startTimeSlider.value);
            const end = parseFloat(endTimeSlider.value);

            alert(`Trimmed video will start at ${start} seconds and end at ${end} seconds. (Note: This is just a preview of trimming, not an actual video file trim.)`);
        });

        // Update the max values of the sliders based on the video duration
        video.addEventListener('loadedmetadata', function() {
            const duration = video.duration;
            startTimeSlider.max = duration;
            endTimeSlider.max = duration;
        });
    </script>

</body>
</html>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Click the Box Game</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            background-color: #f4f4f4;
            margin: 0;
            padding: 0;
            display: flex;
            justify-content: center;
            align-items: center;
            height: 100vh;
        }
        #gameContainer {
            width: 600px;
            height: 400px;
            background-color: #333;
            position: relative;
            overflow: hidden;
            border-radius: 10px;
        }
        #gameBox {
            width: 50px;
            height: 50px;
            background-color: red;
            position: absolute;
            top: 0;
            left: 0;
        }
        #score {
            color: white;
            font-size: 20px;
            position: absolute;
            top: 20px;
            left: 20px;
        }
        #startButton {
            padding: 10px 20px;
            font-size: 16px;
            background-color: #333;
            color: white;
            border: none;
            border-radius: 5px;
            cursor: pointer;
            position: absolute;
            bottom: 20px;
            left: 50%;
            transform: translateX(-50%);
        }
        #startButton:hover {
            background-color: #555;
        }
    </style>
</head>
<body>

    <div id="gameContainer">
        <div id="score">Score: 0</div>
        <div id="gameBox"></div>
        <button id="startButton">Start Game</button>
    </div>

    <script>
        // Game variables
        let score = 0;
        let gameBox = document.getElementById('gameBox');
        let scoreDisplay = document.getElementById('score');
        let startButton = document.getElementById('startButton');
        let gameContainer = document.getElementById('gameContainer');

        let gameStarted = false;

        // Function to start or reset the game
        function startGame() {
            score = 0;
            gameStarted = true;
            scoreDisplay.textContent = `Score: ${score}`;
            moveBox(); // Start moving the box
            startButton.style.display = 'none'; // Hide the start button
        }

        // Function to randomly move the box
        function moveBox() {
            if (!gameStarted) return;

            let maxWidth = gameContainer.offsetWidth - gameBox.offsetWidth;
            let maxHeight = gameContainer.offsetHeight - gameBox.offsetHeight;

            let randomX = Math.random() * maxWidth;
            let randomY = Math.random() * maxHeight;

            gameBox.style.left = randomX + 'px';
            gameBox.style.top = randomY + 'px';

            // Update score when box is clicked
            gameBox.addEventListener('click', () => {
                if (gameStarted) {
                    score++;
                    scoreDisplay.textContent = `Score: ${score}`;
                    moveBox(); // Move the box after a click
                }
            });

            // Call moveBox again after a random time between 500ms and 1.5s
            let randomTime = Math.random() * 1000 + 500;
            setTimeout(moveBox, randomTime);
        }

        // Start game when the button is clicked
        startButton.addEventListener('click', () => {
            if (!gameStarted) {
                startGame();
            }
        });
    </script>

</body>
</html>

